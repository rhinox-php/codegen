<codegen>
    <entity name="Route">
        <string-attribute name="Http Method" />
        <string-attribute name="Url Path" />
        <string-attribute name="Controller Class" />
        <string-attribute name="Controller Method" />
    </entity>

    <entity name="Template">
        <object-attribute name="Codegen" class="Rhino\Codegen\Codegen" />
        <array-attribute name="Namespaces" />
        <array-attribute name="Paths" />
    </entity>

    <entity name="Object Attribute">
        <string-attribute name="Class" />
    </entity>
</codegen>
